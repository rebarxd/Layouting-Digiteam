            app:layout_constraintStart_toEndOf="@id/imgProfile"
            app:layout_constraintTop_toBottomOf="@id/name"
            android:layout_marginStart="8dp"
            />

        <androidx.cardview.widget.CardView
            android:id="@+id/cardTwo"
