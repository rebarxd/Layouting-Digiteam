            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginHorizontal="16dp">

                <TextView
                    android:id="@+id/titleTime"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="@string/titleTime"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:textSize="20sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/descTitle"
                    android:textFontWeight="600"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="16dp"
                    android:text="@string/descTime"
                    android:textColor="@color/blue"
                    app:layout_constraintTop_toBottomOf="@id/titleTime"/>

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:id="@+id/parentTime"
                    android:layout_width="match_parent"
                    android:layout_height="38dp"
                    android:layout_marginTop="8dp"
                    app:layout_constraintTop_toBottomOf="@id/descTitle"
                    android:background="@drawable/progresschart">

                    <TextView
                        android:id="@+id/workingTime"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="center|start"
                        android:paddingStart="8dp"
                        android:paddingEnd="0dp"
                        android:background="@color/green_chart"
                        android:text="@string/time"
                        android:textColor="@color/white"
                        android:textStyle="bold"
                        android:textSize="18sp"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/cloak"
                        app:layout_constraintHorizontal_weight="8"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent" />

                    <ImageView
                        android:id="@+id/cloak"
                        android:layout_width="69dp"
                        android:layout_height="26dp"
                        android:layout_marginTop="8dp"
                        android:layout_marginEnd="11dp"
                        android:paddingStart="15dp"
                        android:paddingEnd="0dp"
                        android:scaleType="fitEnd"
                        android:src="@drawable/ic_clock"
                        app:layout_constraintEnd_toEndOf="@id/parentTime"
                        app:layout_constraintTop_toTopOf="parent"/>
                </androidx.constraintlayout.widget.ConstraintLayout>

                <TextView
                    android:id="@+id/descTime"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/descTime2"
                    android:layout_marginTop="8dp"
                    android:textSize="12dp"
                    android:textColor="@color/subgrey"
                    app:layout_constraintTop_toBottomOf="@id/parentTime"/>

                <com.google.android.material.button.MaterialButton
                    android:layout_width="match_parent"
                    app:cornerRadius="10dp"
                    android:backgroundTint="@color/green_chart"
                    android:layout_height="wrap_content"
                    android:text="@string/checkin"
                    android:textColor="@color/white"
                    android:layout_marginTop="24dp"
                    android:textStyle="bold"
                    android:textSize="14dp"
                    app:layout_constraintTop_toBottomOf="@id/descTime"/>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

